{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=3"
  },
  "scripts": {
    "build": "nuxt build",
    "build:dev": "dotenv -c dev nuxt build",
    "build:prod": "dotenv -c prod nuxt build",
    "dev": "dotenv -c dev nuxt dev",
    "dev:prod": "dotenv -c prod nuxt dev",
    "generate": "nuxt generate",
    "generate:dev": "dotenv -c dev nuxt generate",
    "generate:prod": "dotenv -c prod nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "config:cors": "node scripts/configure-storage-cors.js",
    "deploy": "nuxt build && firebase deploy",
    "deploy:with-cors": "nuxt build && node scripts/configure-storage-cors.js && firebase deploy",
    "deploy:preview": "NITRO_PRESET=firebase nuxt build && firebase hosting:channel:deploy $(git rev-parse --abbrev-ref HEAD) --expires 7d",
    "deploy:preview:dev": "dotenv -c dev NITRO_PRESET=firebase nuxt build && firebase use dev && firebase hosting:channel:deploy $(git rev-parse --abbrev-ref HEAD) --expires 7d",
    "emulators": "firebase emulators:start --import=./.firebase-emulator-data --export-on-exit",
    "dev:emulators": "firebase emulators:start --import=./.firebase-emulator-data --export-on-exit & dotenv -c dev FIREBASE_USE_EMULATOR=true nuxt dev",
    "save:preview-url": "node -e \"const branchName = require('child_process').execSync('git rev-parse --abbrev-ref HEAD', {encoding: 'utf8'}).trim(); console.log('Preview URL: https://'+branchName+'--'+require('./firebase.json').hosting.site+'.web.app');\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "@nuxt/content": "^3.5.1",
    "@nuxt/eslint": "1.4.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "0.11.7",
    "@nuxt/types": "^2.18.1",
    "@unhead/vue": "^2.0.3",
    "better-sqlite3": "^11.10.0",
    "eslint": "^9.0.0",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.3.0",
    "firebase-functions": "^6.3.2",
    "firebase-tools": "13.35.0",
    "js-image-compressor": "2.0.0",
    "node-gyp": "^11.2.0",
    "nuxt": "^3.17.3",
    "nuxt-vuefire": "1.0.5",
    "vee-validate": "3.3.9",
    "vue": "^3.5.13",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "^4.5.1",
    "vuefire": "3.2.1",
    "vuetify-nuxt-module": "^0.18.6",
    "vuex": "4.1.0",
    "vuexfire": "3.2.5",
    "zod": "^3.25.4"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vuetify": "^3.8.5"
  },
  "onlyBuiltDependencies": [
    "better-sqlite3"
  ],
  "trustedDependencies": [
    "@firebase/util",
    "@parcel/watcher",
    "better-sqlite3",
    "esbuild",
    "protobufjs",
    "re2",
    "sharp",
    "ufo",
    "unrs-resolver"
  ]
}
